FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      sample;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type            sets;
libs            (sampling);
interpolationScheme cellPoint;
setFormat       csv;

fields          (T rho p);

sets
{
    data
    {
        type    uniform;
        axis    x;
        start   (0 0 0);
        end     (1 0 0);
        nPoints 100;
    }
}


